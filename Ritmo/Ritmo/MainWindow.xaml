        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ritmo"
        mc:Ignorable="d"
        Title="MainWindow" Height="1800" Width="2880">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="67*"/>
            <ColumnDefinition Width="725*"/>
        </Grid.ColumnDefinitions>
        <MediaElement x:Name="CurrentTrackElement" HorizontalAlignment="Left" Height="100" Margin="140.689,128,0,0" VerticalAlignment="Top" Width="100" RenderTransformOrigin="-1.145,1.054" Grid.Column="1"/>
        <MediaElement x:Name="WaitingListToQueueTrackElement" HorizontalAlignment="Left" Height="100" Margin="412.689,128,0,0" VerticalAlignment="Top" Width="100" Grid.Column="1"/>
        <Button x:Name="PlayButton" Content="Play" HorizontalAlignment="Left" Margin="290.689,251,0,0" VerticalAlignment="Top" Width="75" Grid.Column="1"/>

    </Grid>
</Window>
