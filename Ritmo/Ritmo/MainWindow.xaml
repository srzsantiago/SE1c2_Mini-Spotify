        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <MediaElement x:Name="CurrentTrackElement" HorizontalAlignment="Left" Height="100" Margin="208,128,0,0" VerticalAlignment="Top" Width="100" RenderTransformOrigin="-1.145,1.054"/>
